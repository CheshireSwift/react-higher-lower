{
  "name": "react-higher-lower",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "parcel build src/index.html",
    "check-all": "yarn type-check && yarn test && yarn mutate && yarn build",
    "dev": "concurrently -c yellow,magenta,cyan -n server,tests,types npm:watch-serve npm:watch-tests npm:watch-types --default-input-target=tests",
    "mutate": "stryker run",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch-serve": "parcel src/index.html",
    "watch-tests": "jest --watch -u",
    "watch-types": "tsc --noEmit --watch --preserveWatchOutput"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.10",
    "normalize.css": "^8.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-hyperscript-helpers": "^2.0.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-hyperscript": "^3.0.3",
    "@types/react-redux": "^6.0.7",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^3.0.9",
    "awesome-typescript-loader": "4",
    "babel-core": "6",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^4.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.5.0",
    "parcel-bundler": "^1.9.7",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "stryker": "^0.29.1",
    "stryker-api": "^0.21.0",
    "stryker-babel-transpiler": "^0.7.1",
    "stryker-jest-runner": "^1.2.0",
    "stryker-typescript": "^0.13.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3",
    "utility-types": "^2.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.ts",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
